{
  "name": "react-webpack-typescript",
  "version": "2.0.1",
  "description": "Webpack React boilerplate, Typescript and Sass.",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.config.js --mode=development --env dev --color --open",
    "build": "webpack --config=config/webpack.config.js --mode=production --progress",
    "analysis": "webpack --config=config/webpack.config.js --mode=production --profile --json > stats.json"
